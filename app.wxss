/**app.wxss**/
page {
  padding: 0;
  margin: 0;
}
.clearFix:after {
  clear: both;
  content: ".";
  display: block;
  font-size: 0;
  height: 0;
  line-height: 0;
  visibility: hidden;
}

.clearFix {
  clear: both;
  zoom: 1;
}
.fl {
	float: left;
}
.fr {
	float: right;
}
checkbox .wx-checkbox-input{
	/*checkbox css style*/ 
	width: 34rpx;
	height: 34rpx;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked{
	/*checkbox checked css style*/ 
	width: 34rpx;
	height: 34rpx;
	background-color: #005bac;
	border-bottom: 2rpx solid #ededed;
}
.bottomWrap {
  padding: 0 25rpx;
  position: fixed;
  bottom: 30rpx;
  width: 700rpx;
  height: 70rpx;
}

.bottomWrap .bottom {
  width: 100%;
  height: 100%;
}